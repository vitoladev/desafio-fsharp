{
  "version": "0.2.0",
  "configurations": [
    {
      "program": "${workspaceFolder}/src/backend/bin/Debug/net6.0/backend.App.dll",
      "request": "launch",
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      // Configuration ommitted for brevity.
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:5001",
        "ASPNETCORE_DETAILEDERRORS": "1",
        "ASPNETCORE_SHUTDOWNTIMEOUTSECONDS": "3",
        "DATABASE_CONNECTION_STRING": "Server=localhost;Port=5432;User Id=postgres;Password=postgres;Database=postgres"
      },
    }
  ]
}